{
  "name": "주식현재가 시세",
  "version": "v1_국내주식-008",
  "method": "GET",
  "productionDomain": "https://openapi.koreainvestment.com:9443",
  "developmentDomain": "https://openapivts.koreainvestment.com:29443",
  "url": "/uapi/domestic-stock/v1/quotations/inquire-price",
  "format": "JSON",
  "contentType": "application/json; charset=UTF-8",
  "request": {
    "headers": [
      { 
        "code": "content-type",
        "name": "컨텐츠 타입",
        "type": "String",
        "required": "N",
        "length": "40",
        "description": "application/json; charset=utf-8"
      }
    ],
    "queries": [
      { 
        "code": "FID_COND_MRKT_DIV_CODE",
        "name": "FID 조건 시장 분류 코드",
        "type": "String",
        "required": "Y",
        "length": "2",
        "description": "J : 주식, ETF, ETN"
      },
      { 
        "code": "FID_INPUT_ISCD",
        "name": "FID 입력 종목코드",
        "type": "String",
        "required": "Y",
        "length": "12",
        "description": "종목번호 (6자리) ETN의 경우, Q로 시작 (EX. Q500001)"
      }
    ]
  },
  "response": {
    "headers": [
      { 
        "code": "content-type",
        "name": "컨텐츠 타입",
        "type": "String",
        "required": "Y",
        "length": "40",
        "description": "application/json; charset=utf-8"
      }
    ],
    "body": [
      { 
        "code": "rt_cd",
        "name": "성공 실패 여부",
